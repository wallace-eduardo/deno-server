{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Launch Program",
            "type": "node",
            "program": "${workspaceFolder}/main.ts",
            "cwd": "${workspaceFolder}",
            "env": {},
            "runtimeExecutable": "/Users/wallace/.dvm/bin/deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--inspect-wait",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        },
        {
            "name": "Debug Deno (node)",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/main.ts",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--inspect-brk",
                "--allow-all"
            ],
            "attachSimplePort": 9229,
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
        ,
        {
            "name": "Debug Deno Tests",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/main.test.ts",
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "test",
                "--inspect-brk",
                "--allow-all"
            ],
            "attachSimplePort": 9229,
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
    ]
}